 <script>
 /* Напишите функцию pow(x,n), которая возвращает x в степени n. Иначе говоря, умножает x на себя n раз и возвращает результат.
 */
"use strict" 
function pow(chislo,stepen){
let result = chislo**stepen;

return result;
}
let chislo = prompt("Введите число x:" ,"");
let stepen = prompt("введите степень n:" ,"");
if (stepen < 1){
alert("число не натуральное, введите натуральное число")
}else{
alert(pow(chislo, stepen));
}

</script>
